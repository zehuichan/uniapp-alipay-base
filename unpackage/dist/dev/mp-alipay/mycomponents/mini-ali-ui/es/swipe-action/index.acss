.am-swipe{position:relative;overflow:hidden}.am-swipe-content{position:relative;transition:all 250ms}.am-swipe-right{position:absolute;top:1px;bottom:1px;display:flex;right:4px;transition:all .1s 0s ease-out}.am-swipe-btn{padding:0 12px;padding:0 var(--am-swipeAction-btn-paddingLR,12px);justify-content:center;align-items:center;display:flex;color:#fff;color:var(--am-swipeAction-btn-color,#fff)}.am-swipe-btn-text{height:1em;overflow:hidden;line-height:1;line-height:var(--am-swipeAction-btn-lineHeight,1)}.am-swipe-btn-delete{background-color:#ff3b30;background-color:var(--am-swipeAction-delete,#ff3b30)}.am-swipe-btn-edit{background-color:#ccc;background-color:var(--am-swipeAction-edit,#ccc)}.am-swipe-btn-other{background-color:#1677ff;background-color:var(--am-swipeAction-other,#1677ff)}.am-swipe-right-radius{overflow:hidden}.am-swipe-right-radius .am-swipe-btn{margin-right:1px}.am-swipe-right-radius .am-swipe-btn:nth-child(2){border-radius:8px 0 0 8px;border-radius:var(--am-swipeAction-item-radius,8px) 0 0 var(--am-swipeAction-item-radius,8px)}.am-swipe-right-radius .am-swipe-btn:last-child{margin-right:0;border-radius:0 8px 8px 0;border-radius:0 var(--am-swipeAction-item-radius,8px) var(--am-swipeAction-item-radius,8px) 0}.am-swipe-right-radius .am-swipe-btn:nth-child(2):last-child{border-radius:8px;border-radius:var(--am-swipeAction-item-radius,8px)}.am-swipe-movable-area .am-swipe-content{transition:none}.am-swipe .am-list-item{height:100%;box-sizing:border-box}.am-swiping .am-swipe-content{transition:none}